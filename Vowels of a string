//program to find the vowels of a string

#include<stdio.h>
void main()
{
  char s[1000];
    int c=0,count=0;
    printf("Enter the string:\n");
    gets(s);
    while(s[c]!='\0')
    {
        if(s[c]=='a'||s[c]=='e'||s[c]=='i'||s[c]=='o'||s[c]=='u')
            count++;
        c++;
       
    }
    printf("Vowels = %d",count);
    return 0;
    
 }
